page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.item-deviceList {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 15px;
  background: white;
}

.deviceName {
  margin-left: 30px;
  font-size: 0.9rem;
}

.deviceIcon {
  width: 80rpx;
  height: 80rpx;
}

.deviceInto {
  width: 10px;
  height: 10px;
  border-top: 2px solid #0AAAF6;
  border-right: 2px solid #0AAAF6;
  position: absolute;
  right: 20px;
  transform: rotate(45deg);

}
